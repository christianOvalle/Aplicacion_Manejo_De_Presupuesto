@model TipoCuenta

@{
    ViewData["Title"] = "BorraTipoCuenta";
}

<h3 class="mb-3">Eestas seguro que quieres borra el tipo cuenta @Model.Nombre</h3>

<form asp-action="BorraTipoCuenta">
    <input type="hidden" asp-for="Id" />
    <button type="submit" class="btn btn-danger">Borrar</button>
    <a class="btn btn-secondary" asp-action="Index">Cancelar</a>

</form>